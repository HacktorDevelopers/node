<% include ../partials/header %>
<div class="container">
<table class="table">
	<h1>List Of Databases</h1>
	<%= "Number of Databaes " + users.length %>
	<tr>
		<th>id</th>
		<th>Database Name</th>
		<th>Action</th>
	</tr>
	<% for(var i = 0; i < users.length; i++){ %>
		<tr>
			<td><%= i+1 %></td>
			<td><%= users[i].Database %></td>
			<td><a class="btn btn-primary">View</a><a class="btn btn-danger">Delete</a></td>
		</tr>
	<% } %>
</table>
</div>
<% include  %>